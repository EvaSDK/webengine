<VirtualHost *:443>
        <Location "/">
                SetHandler python-program
                PythonHandler django.core.handlers.modpython
                SetEnv DJANGO_SETTINGS_MODULE webengine.settings
                PythonOption django.root /
                PythonDebug On
                PythonPath "['@sysconfdir@', '@webenginesysconfdir@'] + sys.path"
        </Location>
</VirtualHost>
